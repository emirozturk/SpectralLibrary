<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Flask Proxy" stopProcessing="true">
          <match url="^(auth|categories|dashboard|users|folders|spectfile|subcategory)(/.*)?$" />
          <action type="Rewrite" url="http://127.0.0.1:5000/{R:0}" />
        </rule>
      </rules>
    </rewrite>
    <proxy enabled="true" preserveHostHeader="true" />
  </system.webServer>
</configuration>
